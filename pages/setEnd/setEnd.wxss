/* pages/setEnd/setEnd.wxss */
.userBox{
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-bottom: 1rpx solid #ccc;
}

.userPhoto{
  width: 120rpx;
  height: 150rpx;
  text-align: center;
  align-items: center;
  color: #3c78ff;
  font-size: 24rpx;
}

.userBox .user{
  width: 80rpx;
  height: 80rpx;
  margin-top: 10rpx;
  border: 6rpx solid #3c78ff;
  border-radius: 50%;
}

.add{
  width: 60rpx;
  height: 60rpx;
  margin-top: 24rpx;
  margin-bottom: 16rpx;
  background: none;
  border: 0;
}

.addFont{
  font-size: 24rpx;
  color: #808080;
}

.itemsBox{
  width: 100%;
  height: 110rpx;
  background: #fff;
  text-align: center;
}

.items{
  width: 30%;
  height: 100rpx;
  border: 1rpx solid #ccc;
  border-radius: 8rpx;
  margin: 10rpx;
  font-size: 36rpx;
}

.items .title{
  color: #808080;
  font-size: 20rpx;
  margin-top: 14rpx;
}

.updateTime{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  color: #808080;
  font-size: 20rpx;
  border-bottom: 1rpx solid #ccc;
}


#map{
  width: 100%;
  height: 800rpx;
}
.speakBox{
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
}

button{
  width: 90%;
}